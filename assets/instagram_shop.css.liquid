/* banner */
.content--parallax-sm1{
  height: 570px;
}
.content--parallax-sm1 .parallax-text .font88 {
  font-size: 88px;
  line-height: 1;
  letter-spacing: 0em;
  margin-top: 51px;
}
.content--parallax-sm1 .parallax-text .font22{
  line-height: 1.5;
  letter-spacing: 0.02em;
  width: 54%;
  display: inline-block;
  margin: 0 auto;
}
.content--parallax-sm1 .divider--xs {
  height: 6px;
}
@media (max-width: 1024px) {
  .content--parallax-sm1 .parallax-text .font88{
    font-size: 50px;
    margin-top: 0;
  }
  .content--parallax-sm1 .parallax-text .font22{
    font-size: 18px;
    width:80%;
  }
}
@media (max-width: 767px) {
  .content--parallax-sm1{
    height: inherit;
  }
  .content--parallax-sm1 .parallax-text .font88{
    font-size: 30px;
  }
  .content--parallax-sm1 .parallax-text .font22{
    font-size: 15px;
    width:90%;
  }
}
/* /banner */
/* snptwdgt */
.snptwdgt__ftr,
.fauximg-bin .snptico,
.public-modal .modal-left-column .snptico-shopping-bag{
  display: none;
}
.snptwdgt-container-liner .fauximg-bin,
.snptwdgt-container .snptwdgt__item .snpt__fauxim img{
  backface-visibility: hidden;
}
.snptwdgt-container .snptwdgt__item .snpt__fauximg{
  -webkit-transition: opacity 0.3s, -webkit-transform 0.3s;
  transition: opacity 0.3s, transform 0.3s;
  -webkit-transform: scale(1);
  transform: scale(1);
  backface-visibility: hidden;
}
.snptwdgt-container .snptwdgt__item:hover .snpt__fauximg{
  -webkit-transform: scale(1.05);
  transform: scale(1.05);
}
.widget-hotspot:hover{
  background: {{ settings.color_base }};
  text-decoration: none;
}
.hotspot.hotspot--highlighted, .hotspot:hover{
  background: {{ settings.color_base }};
  text-decoration: none;
}
.hotspot{
  backface-visibility: hidden;
  -moz-transition: all 0.3s linear;
  -o-transition: all 0.3s linear;
  -webkit-transition: all 0.3s linear;
  text-decoration: none;
}
.widget-hotspot{
  background: {{ settings.color_base }};
  color: #FFF;
  border-width: 3px;
}
/* /snptwdgt */

/* loader */
.loadr-arc-rotate-move .loader:before{
  border-color: rgba(31,192,160,.155);
  opacity: .3;
}
.loadr-arc-rotate-move .loader:after{
  border-bottom-color: {{ settings.color_base }};
}
/* /loader */

