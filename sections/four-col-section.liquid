<!--snackswork start-->
<section class="snackswork define-float">
  <div class="container">
    <div class="main-four-section">
      <div class="col-md-6 col-custom for-first">
        <div class="snack-black">
          <img src="{{ section.settings.fstcol-image | img_url: "" }}" class="snack-img">
          <div class="snack-post"> <span>{{ section.settings.fstcol-text }}</span>
          
          </div>
        </div>
      </div>
      <div class="col-md-6 col-custom for-second">
        <div class="snack-black">
           <img src="{{ section.settings.sstcol-image | img_url: "" }}" class="snack-img">
          <div class="snack-post"> <span>{{ section.settings.sstcol-text }}</span>  </div>
        </div>
      </div>
      <div class="col-md-6 col-custom for-three">
        <div class="snack-black">
           <img src="{{ section.settings.tstcol-image | img_url: "" }}" class="snack-img">
          <div class="snack-post"> <span>{{ section.settings.tstcol-text }}</span>  </div>
        </div>
      </div>
      <div class="col-md-6 col-custom for-four">
        <div class="snack-black">
           <img src="{{ section.settings.fostcol-image | img_url: "" }}" class="snack-img">
          <div class="snack-post"> <span>{{ section.settings.fostcol-text }}</span> </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!--snackswork closed-->
