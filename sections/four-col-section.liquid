      "label": "Second Col Image"
    },
    {
      "type": "text",
      "id": "sstcol-text",
      "label": "Second Col Title"
      
    },
    {
      "type": "image_picker",
      "id": "tstcol-image",
      "label": "Third Col Image"
    },
    {
      "type": "text",
      "id": "tstcol-text",
      "label": "Third Col Title"
      
    },
    {
      "type": "image_picker",
      "id": "fostcol-image",
      "label": "Fourth Col Image"
